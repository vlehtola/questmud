inherit "room/room";

reset(arg) {
  if(arg) return;
  add_exit("inn","/world/city/inn");
  add_exit("cs","/world/city/cs");
  add_exit("post","/world/city/post");
  add_exit("east","/wizards/archsin/livingroom");
  short_desc = "All your exp are belong to me.";
  long_desc = "You DARE to peek in here?\n"; 
  property = allocate(1);
  property[0] = "no_summon";
}







