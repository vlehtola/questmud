inherit "obj/monster";
reset(arg) {
    ::reset(arg);
    if(arg) { return; }
    set_level(10);
    set_name("servant");
    set_alias("woman");
    set_short("Woman servant cleaning floors");
    set_long("This old woman looks like she has been cleaning floors forever.\n"+ 
             "She has long white hair and she is wearing a long grey robes.\n"+ Small 
             "wrinkles can be spotted on her face. Long broom moves smoothly\n"+ 
             "in her hands. This woman is really a professional\n");
    set_gender(2);
    set_al(10);
    set_aggressive(0);
    set_race("human");
}


