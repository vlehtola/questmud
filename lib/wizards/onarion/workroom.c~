inherit "room.h";

reset(arg) {
    if (arg) { return 0; }
    ::reset(arg);
    items = allocate(2);
    items[0] = "sign";
    items[1] = "A big sign that says 'it works'.\n";
}

ONE_EXIT("world/city/inn","inn",
"Onarion's workroom.",
"This is Onarion's workroom. Maybe you should not disturb him\n" +
"while he is working.\n", 1)


